    <script>
        async function buyItem(i, t, p) {
            const data = await fetch("/shop/buy", {
                method: "POST",
                headers: {
                    "Content-Type": "application/json"
                },
                body: JSON.stringify({
                    t: t,
                    i: i,
                    p: p
                })
            })

            const content_type = data.headers.get("Content-Type");

            // –ù—É –¥–∞, —Ç–∞–∫–æ–π –≤–æ—Ç –∫–æ—Å—Ç—ã–ª—å
            if (content_type ? content_type.match("text/html") : false) {
                document.open()
                document.write(await data.text())
                document.close()
            }

            if (data.status == 200 || !data.redirected)
                document.location = await data.text();

            if (data.redirected)
                document.location = data.url;
        }
